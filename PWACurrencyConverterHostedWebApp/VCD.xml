<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CurrencyConversionCommandSet_en-us">
    <CommandPrefix>Currency Converter</CommandPrefix>
    <Example>Currency rates : USD, INR</Example>

    <Command Name="Open_Currency_Converter">
      <Example>Open</Example>
      <ListenFor>{open}</ListenFor>
      <Feedback>Opening</Feedback>
      <Navigate Target="/index.htm" />
    </Command>
    
    <PhraseList Label="open">
      <Item>Open</Item>
      <Item>Start</Item>
    </PhraseList>
 
  </CommandSet>
</VoiceCommands>